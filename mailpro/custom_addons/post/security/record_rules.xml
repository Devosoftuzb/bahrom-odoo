<odoo>
    <record id="promo_code_manager_rule" model="ir.rule">
        <field name="name">Promo Code Manager Rule</field>
        <field name="model_id" ref="post.model_post_promo_code"/>
        <field name="domain_force">[('create_uid', '=', user.id)]</field>
        <field name="groups" eval="[(4, ref('group_promo_code_manager'))]"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
</odoo>
