<odoo>
    <record id="eduhub_administrator" model="ir.module.category">
        <field name="name">EduHub Administrator Category</field>
    </record>

    <record id="eduhub_admin" model="ir.module.category">
        <field name="name">Eduhub Admin Category</field>
    </record>

    <record id="group_eduhub_administrator" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="eduhub_administrator"/>
    </record>

    <record id="group_eduhub_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="eduhub_admin"/>
    </record>
</odoo>
